      <ImageView fitHeight="180.0" fitWidth="250.0" layoutX="85.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../image/user_img.png" />
         </image>
      </ImageView>
      <Label layoutX="52.0" layoutY="263.0" text="User Name" />
      <TextField fx:id="txt_username" layoutX="148.0" layoutY="259.0" />
      <Label layoutX="52.0" layoutY="306.0" text="Password" />
      <Button fx:id="btn_login" layoutX="147.0" layoutY="342.0" mnemonicParsing="false" onAction="#btn_login_click" prefWidth="150.0" text="login" />
      <PasswordField fx:id="txt_password" layoutX="149.0" layoutY="302.0" />
   </children>
</AnchorPane>
